[(Stack OS Rize)
    [(width) 1000]
    [(height) 570]

    [(layout)]

    #Main code of kernel #
    [(code) (
        # Run system call #
        (cmd) var cmd eval (result) var

        # JavaScript code to show result #
        (document.getElementById("result").innerHTML = "{}";)
        ({}) result replace
    )]
] (window) var

window [
    # Read user-interface #
    (./system/ui.html) read-file
] instance (kernel) var

# Move home directory #
(home) cd

kernel gui
